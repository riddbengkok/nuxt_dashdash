{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production nuxt build",
    "dev": "nuxt dev",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "postinstall": "nuxt prepare",
    "deploy": "nuxt generate && gh-pages -d .output/public",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true nuxt build",
    "analyze:build": "pnpm run generate && node scripts/analyze.js",
    "analyze:detailed": "pnpm run generate && node scripts/detailed-analyze.js",
    "preview": "node scripts/preview.js",
    "preview:local": "cross-env NODE_ENV=production PREVIEW=true nuxt generate && npx serve .output/public",
    "preview:prod": "pnpm run generate && npx serve .output/public"
  },
  "dependencies": {
    "firebase": "^11.10.0",
    "nuxt": "^3.17.6",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@heroicons/vue": "^2.2.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.11",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.46",
    "gh-pages": "^6.3.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
