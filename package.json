{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "deploy": "npm run generate && gh-pages -d .output/public",
    "analyze": "nuxt build",
    "analyze:build": "npm run generate && node scripts/analyze.js",
    "analyze:detailed": "npm run generate && node scripts/detailed-analyze.js",
    "preview": "node scripts/serve-preview.cjs",
    "preview:local": "npm run generate && node scripts/serve-preview.cjs",
    "preview:prod": "npm run generate && node scripts/serve-preview.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@nuxt/ui": "^3.2.0",
    "firebase": "^11.10.0",
    "nuxt": "^3.17.6",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/eslint-config": "^1.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.46",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.3.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
